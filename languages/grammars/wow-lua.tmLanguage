			<key>name</key>
			<string>meta.function.lua</string>
			<key>match</key>
			<string>(?&lt;=^|\s|;)(?:(function)\s*(?:([a-zA-Z_]\w*)([.:])([a-zA-Z_]\w*)|([a-zA-Z_]\w*))(\()([^)]*)(\)))</string>
