		<dict>
			<key>name</key>
			<string>comment.block.lua</string>
			<key>begin</key>
			<string>--\[(=*)\[</string>
			<key>end</key>
			<string>\]\1\]</string>
			<key>captures</key>
			<dict>
				<key>0</key>
