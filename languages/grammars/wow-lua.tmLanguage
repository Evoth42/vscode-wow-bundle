<plist version="1.0">
<!--
	- Lua tmLanguage file by alfredbaudisch : https://forum.sublimetext.com/t/a-fix-to-the-lua-language-stored-functions-in-go-to-symbol/8790
	- WoW functions semi-automatically generated after Wowpedia's raw wikitext : http://wow.gamepedia.com/World_of_Warcraft_API?action=raw
-->
<dict>
	<key>name</key>
	<string>lua</string>
	<key>fileTypes</key>
	<array>
		<string>lua</string>
	</array>
	<key>foldingStartMarker</key>
	<string>^\s*\b(?:(?:local\s+)?function|if|for)\b|{[ \t]*$|\[=*\[</string>
	<key>foldingStopMarker</key>
	<string>\bend\b|^\s*}|\]=*\]</string>

	<key>patterns</key>
	<array>
		<!-- These need to come *before* regular string patterns -->
		<dict>
			<key>include</key>
			<string>#wow-string-parameters</string>
		</dict>
		<dict>
			<key>include</key>
			<string>#wow-widget-handlers</string>
		</dict>
		<dict>
			<key>include</key>
			<string>#wow-event-names</string>
		</dict>

		<!-- Function declaration -->
		<dict>
			<key>name</key>
			<string>meta.function.lua</string>
			<key>match</key>
			<string>(?&lt;=^|\s|;)(?:(?:([a-zA-Z_]\w*)([.:])([a-zA-Z_]\w*)|([a-zA-Z_]\w*))\s*(=)\s*(function)(\()([^)]*)(\)))</string>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.scope.lua</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>keyword.operator.lua</string>
				</dict>
				<key>3</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.lua</string>
				</dict>
				<key>4</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.lua</string>
				</dict>
				<key>5</key>
				<dict>
					<key>name</key>
					<string>keyword.operator.lua</string>
				</dict>
				<key>6</key>
				<dict>
					<key>name</key>
					<string>keyword.control.lua</string>
				</dict>
				<key>7</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.parameters.begin.lua</string>
				</dict>
				<key>8</key>
				<dict>
					<key>name</key>
					<string>variable.parameter.function.lua</string>
				</dict>
				<key>9</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.parameters.end.lua</string>
				</dict>
			</dict>
		</dict>
		<dict>
			<key>name</key>
			<string>meta.function.lua</string>
			<key>match</key>
			<string>(?&lt;=^|\s|;)(?:(function)\s*(?:([a-zA-Z_]\w*)([.:])([a-zA-Z_]\w*)|([a-zA-Z_]\w*))(\()([^)]*)(\)))</string>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>keyword.control.lua</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.scope.lua</string>
				</dict>
				<key>3</key>
				<dict>
					<key>name</key>
					<string>keyword.operator.lua</string>
				</dict>
				<key>4</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.lua</string>
				</dict>
				<key>5</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.lua</string>
				</dict>
				<key>6</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.parameters.begin.lua</string>
				</dict>
				<key>7</key>
				<dict>
					<key>name</key>
					<string>variable.parameter.function.lua</string>
				</dict>
				<key>8</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.parameters.end.lua</string>
				</dict>
			</dict>
		</dict>

		<!-- Numbers -->
		<dict>
			<key>name</key>
			<string>constant.numeric.lua</string>
			<key>match</key>
			<string>(?&lt;![\d.])\s0x[a-fA-F\d]+|\b\d+(\.\d+)?([eE]-?\d+)?|\.\d+([eE]-?\d+)?</string>
		</dict>

		<!-- Strings -->
		<dict>
			<key>name</key>
			<string>constant.language.quoted.lua</string>
			<key>match</key>
			<string>(['"])(nil|number|string|table|boolean|function|userdata|thread|kv?|vk?)\1</string>
		</dict>
		<dict>
			<key>name</key>
			<string>string.quoted.single.lua</string>
			<key>begin</key>
			<string>'</string>
			<key>beginCaptures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.string.begin.lua</string>
				</dict>
			</dict>
			<key>end</key>
			<string>'</string>
			<key>endCaptures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.string.end.lua</string>
				</dict>
			</dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>constant.character.escape.lua</string>
					<key>match</key>
					<string>\\([abfnrtv\\"']|\r?\n|\n\r?|\d{1,3})</string>
				</dict>
				<dict>
					<key>name</key>
					<string>constant.character.escape.lua</string>
					<key>match</key>
					<string>\\[xX][0-9a-fA-F]{1,2}</string>
				</dict>
				<dict>
					<key>name</key>
					<string>constant.character.escape.lua</string>
					<key>match</key>
					<string>\\u\{[0-9a-fA-F]{1,7}\}</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>name</key>
			<string>string.quoted.double.lua</string>
			<key>begin</key>
			<string>"</string>
			<key>beginCaptures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.string.begin.lua</string>
				</dict>
			</dict>
			<key>end</key>
			<string>"</string>
			<key>endCaptures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.string.end.lua</string>
				</dict>
			</dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>constant.character.escape.lua</string>
					<key>match</key>
					<string>\\([abfnrtv\\"']|\r?\n|\n\r?|\d\d?\d?)</string>
				</dict>
				<dict>
					<key>name</key>
					<string>constant.character.escape.lua</string>
					<key>match</key>
					<string>\\[xX][0-9a-fA-F][0-9a-fA-F]</string>
				</dict>
				<dict>
					<key>name</key>
					<string>constant.character.escape.lua</string>
					<key>match</key>
					<string>\\u\{[0-9a-fA-F]{,7}\}</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>begin</key>
			<string>(?&lt;!--)\[(=*)\[</string>
			<key>beginCaptures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.string.begin.lua</string>
				</dict>
			</dict>
			<key>end</key>
			<string>\]\1\]</string>
			<key>endCaptures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.string.end.lua</string>
				</dict>
			</dict>
			<key>name</key>
			<string>string.quoted.other.multiline.lua</string>
		</dict>

		<!-- Comments -->
		<dict>
			<key>name</key>
			<string>comment.block.lua</string>
			<key>begin</key>
			<string>--\[(=*)\[</string>
			<key>end</key>
			<string>\]\1\]</string>
			<key>captures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.comment.lua</string>
				</dict>
			</dict>
		</dict>
		<dict>
			<key>name</key>
			<string>comment.line.double-dash.lua</string>
			<key>match</key>
			<string>(--)(?!\[\[).*$\n?</string>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.comment.lua</string>
				</dict>
			</dict>
		</dict>

		<!-- Keywords -->
		<dict>
			<key>match</key>
			<string>\b(break|do|else|for|if|elseif|return|then|repeat|while|until|end|function|local|in)\b</string>
			<key>name</key>
			<string>keyword.control.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b(false|nil|true|_G|_VERSION|math\.(pi|huge))\b</string>
			<key>name</key>
			<string>constant.language.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b(self)\b</string>
			<key>name</key>
			<string>variable.language.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\+|-|%|#|\*|\/|\^|==?|~=|&lt;=?|&gt;=?|(?&lt;!\.)\.{2,3}(?!\.)|;|,</string>
			<key>name</key>
			<string>keyword.operator.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b(and|or|not)\b</string>
			<key>name</key>
			<string>keyword.operator.logical.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b(assert|collectgarbage|dofile|error|getfenv|getmetatable|ipairs|loadfile|loadstring|module|next|pairs|pcall|rawequal|rawget|rawset|reqlibraryre|select|setfenv|setmetatable|tonumber|tostring|type|unpack|xpcall)(?=\s*\()\b</string>
			<key>name</key>
			<string>support.function.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b(__(index|newindex|mode|call|metatable|tostring|len|gc|unum|add|sub|mul|div|mod|pow|concat|eq|lt|le))\b</string>
			<key>name</key>
			<string>support.function.metamethod.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b(date|time|bit\.(bnot|band|bor|bxor|lshift|rshift|arshift|mod)|coroutine\.(create|resume|running|status|wrap|yield)|math(\.(abs|acos|asin|atan2?|ceil|cosh?|deg|exp|floor|fmod|frexp|ldexp|log|log10|max|min|modf|pow|rad|random|randomseed|sinh?|sqrt|tanh?))?)\b</string>
			<key>name</key>
			<string>support.function.library.lua</string>
		</dict>
		<dict>
			<!-- string and table libs: allow both simple and OO-style calls, eg. string.format(fmt, ...) and fmt:format(...) -->
			<key>match</key>
			<string>(?&lt;![^.]\.|:)((string\.|:)(byte|char|dump|find|format|gmatch|gsub|len|lower|match|rep|reverse|sub|upper)|(table\.|:)(concat|insert|maxn|pack|remove|sort|unpack))\b</string>
			<key>name</key>
			<string>support.function.library.lua</string>
		</dict>
		<dict>
			<!-- 'this' is not realy an illegal keyword but is considered bad practice, so let's warn about it -->
			<key>match</key>
			<string>\b(this)\b</string>
			<key>name</key>
			<string>invalid.illegal.lua</string>
		</dict>
		<dict>
			<key>match</key>
			<string>(?&lt;![^.]\.|:)((table\.|:)([gs]etn|foreachi?)|(string\.|:)gfind)\b</string>
			<key>name</key>
			<string>invalid.deprecated.lua</string>
		</dict>

		<!-- Blizzard's Lua language extensions -->
		<dict>
			<key>match</key>
			<string>\b(scrub|strlenutf8|strcmputf8|strconcat|strjoin|strsplit|strtrim|wipe)\b</string>
